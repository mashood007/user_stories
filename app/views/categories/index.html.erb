<div class="container">
    <%= link_to "New Category", new_category_path, class: "btn btn-link"%>
    <table class="table table-bordered">
    <%@categories.each_with_index do |category, index|%>
        <tr>
        <td><%= index+1%></td>
        <td>
            <%= category.name%>
        </td>
        </tr>
    <%end%>
    </table>
</div>